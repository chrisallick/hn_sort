{
    "name": "HackerNews",
    "description": "Three sort options for HackerNews.",
    "version": "2.4",
    "manifest_version": 2,
    "content_scripts": [{
        "matches": ["https://news.ycombinator.com/"],
        "js": ["jq.2.min.js", "main.js"]
    }],
    "permissions": [
        "tabs", "http://*/*", "https://*/*", "storage"
    ],
    "browser_action": {
        "default_icon": "icon.png"
    },
    "icons": {
        "64": "icon64.png"
    }
}